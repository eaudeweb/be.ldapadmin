<strong>Roles</strong> <br/>
<div tal:repeat="role view/roles"> ~ <span tal:content="role" />
    <ul>
        <li tal:repeat="d python:view.entry['data']">
            <tal:mt tal:define="old_mt python:d['old_membership_type'];new_mt python:d['new_membership_type']" tal:condition="new_mt">
                (<span tal:replace="old_mt">old_mt</span> => <span tal:replace="new_mt">new_mt</span>)
            </tal:mt>
        </li>
    </ul>
</div>
